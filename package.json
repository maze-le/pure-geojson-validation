{
  "name": "pure-geojson-validation",
  "version": "0.2.3",
  "description": "A geojson validator and coordinate checker with purify-ts support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "npm run clean:dist && npm run clean:index",
    "clean:dist": "rimraf dist",
    "clean:index": "rimraf index.*",
    "build": "npm run clean && tsc",
    "test": "jest",
    "coverage": "npx jest --coverage",
    "coverage/": "npx jest --coverage",
    "prepublishOnly": "npm run clean && tsc && rollup -c"
  },
  "keywords": [
    "geojson",
    "validation",
    "purify-ts"
  ],
  "author": "Mathias Vonende",
  "license": "MIT",
  "dependencies": {
    "@types/geojson": "^7946.0.7",
    "purify-ts": "^0.16.1",
    "rollup-plugin-dts": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
